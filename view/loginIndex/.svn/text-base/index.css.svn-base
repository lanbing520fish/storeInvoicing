.content{
	width:100%;
	height:494px;
	background: #9563c7;
}
.cnt{
	width:1200px;
	height:494px;
	margin:0 auto;
	background: url(../../images/login_bg.jpg) center 0 no-repeat;
}
.fn-login{
	width:360px;
	height:363px;
	float:right;
	margin:60px 120px 0 0;
	border-radius: 8px;
	background: #f8f8f8;
	overflow: hidden;
}
.user{
	width:360px;
	height:44px;
	line-height: 44px;
	background: #464e5d;
	font-size:19px;
	color:#fff;
	font-weight:bold;
}
.user img{
	margin:9px 10px 0 17px;
	vertical-align: top;
}
.lg{
	width:328px;
	margin:0 auto;
	position: relative;
}
.lg .top{
	position: relative;
	width:328px;
	height:46px;
}
.lg .top .wrds{
	width:86px;
	float:left;
	color:#aeaeae;
	font-size:12px;
	margin:19px 0 0 1px;
	text-align: center;
}
.lg .top .change{
	float:right;
	width:130px;
	height:24px;
	line-height: 24px;
	border:1px solid #c9c9c9;
	margin:10px 17px 0 0;
	color:#272727;
	font-weight:bold;
	text-align: center;
	border-radius:5px;
}
.lg .top .change i{
	color:#bf9be2;
	margin-right:5px;
	vertical-align: top;
	font-size:19px;
}
.lg .top .change img{
	margin-left: 5px;
}
.line{
	position:absolute;
	background: url(../../images/line-arrow.png) no-repeat;
	width:344px;
	height:10px;
	left:-17px;
	bottom:0px;
}
.lig{
	width:294px;
	height:34px;
	margin:18px 0 0 16px;
	position: relative;
	/*overflow: hidden;	*/
}
.lig input{
	width:294px;
	height:32px;
	line-height: 32px;
	text-indent: 40px;
	border:1px solid #c9c9c9;
	border-radius:20px;
    outline: 0;
}
.lig.yzm{
	width:166px;
}
.lig.yzm input{
	width:166px;
}
.yzm-img{
	position: absolute;
	left:198px;
	top:170px;
	width:110px;
	height:34px;
	cursor:pointer;
}
.lig i{
	text-align: center;
	position: absolute;
	width:34px;
	line-height:34px;
	background: #fafafa;
	color:#a96ce3;
	border:1px solid #c9c9c9;
	top:0;
	left:0;
	font-size: 18px;
	border-radius:20px;
	border-top-right-radius: 0;
    border-bottom-right-radius: 0;
}
.tel-lg .lig{
	margin-top:20px;
}
.tel-lg .lig i{
	font-size: 22px;
	line-height:34px;
}
.lig.yzm i{
	font-size: 22px;
}
.btns{
	margin-top:18px;
	text-align:center;
}
.tel-lg .btns{
	margin-top:20px;
}
.zh-lg .err{
	position:absolute;
	font-size:12px;
	color:#ff0606;
	top:276px;
	left:20px;
	width:228px;
	overflow: hidden;
}
.tel-lg .err{
	position:absolute;
	font-size:12px;
	color:#ff0606;
	top:230px;
	left:20px;
}
.login{
	width:166px;
	line-height: 32px;
	font-size:14px;
	color:#fff;
	border:1px solid #8f60bf;
	background: #a96ce3;
	border-radius:20px;
	margin-right:8px;
}
.reseting{
	width:117px;
	line-height: 32px;
	font-size:14px;
	border-radius:20px;
	border:1px solid #c39ceb;
	background: #eddaff;
	color:#8233d0;
}
.forget{
	float: right;
	color:#463b3b;
	margin:16px 16px 0 0;
}
.telyzm{
	position: absolute;
	left:198px;
	top:122px;
	width:118px;
	height:34px;
	line-height: 28px;
	text-align:center;
	border:1px solid #cecece;
	background: #f4f4f4;
	color:#6b6b6b;
	font-size:14px;
	border-radius:20px;
}
.footer{
	width:100%;
	height:60px;
	line-height: 60px;
	font-size:12px;
	color:#373737;
	text-align:center;
}
